<section class="horz-stripe">&nbsp;</section>
<div class="pg pg-songs">
  <ul>
    {{#each this}}
    <li class="entry song" data-id="{{ id }}">
      <header>
        <a href="#music/song/{{ title_slug }}/" class="img">
          <img src="{{ art }}" />
        </a>
        <div class="info">
          <h2><a href="#music/song/{{ title_slug }}/">{{ title }}</a></h2>
          <p>{{ byline }} / <a href="#artists/{{ name_slug }}">{{ artist.name }}</a></p>
        </div>
      </header>
    </li>
    {{/each}}
  </ul>
</div>